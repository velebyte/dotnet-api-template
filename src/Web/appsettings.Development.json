{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": { "ServerUrl": "http://localhost:5341" }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithEnvironmentName",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "DotnetTemplate"
    }
  },
  "JwtSettings": {
    "Secret": "top-secret-key-for-jwt",
    "ExpiryMinutes": 60,
    "Issuer": "DotnetTemplate",
    "Audience": "DotnetTemplate"
  },
  "ConnectionStrings": {
    "Dev": "Host=localhost;Port=5433;Username=user;Password=admin;Database=flowerdb"
  }
}
